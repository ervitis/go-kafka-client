{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "name",
    "version"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "minLength": 1,
      "pattern": "^(.*)$"
    },
    "version": {
      "$id": "#/properties/version",
      "type": "string",
      "minLength": 1,
      "pattern": "^(.*)$"
    }
  }
}
